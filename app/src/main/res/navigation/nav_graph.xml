<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/contactsFragment">

    <fragment
        android:id="@+id/editFragment"
        android:name="com.example.a2_contacts.EditFragment"
        android:label="fragment_edit"
        tools:layout="@layout/fragment_edit" >
        <action
            android:id="@+id/action_editFragment_to_contactFragment2"
            app:destination="@id/contactFragment" />
        <action
            android:id="@+id/action_editFragment_to_contactsFragment2"
            app:destination="@id/contactsFragment" />
        <argument
            android:name="contactID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.example.a2_contacts.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" >
        <action
            android:id="@+id/action_contactFragment_to_editFragment2"
            app:destination="@id/editFragment" />
        <argument
            android:name="contactID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/contactsFragment"
        android:name="com.example.a2_contacts.ContactsFragment"
        android:label="fragment_contacts"
        tools:layout="@layout/fragment_contacts" >
        <action
            android:id="@+id/action_contactsFragment_to_editFragment2"
            app:destination="@id/editFragment" />
        <action
            android:id="@+id/action_contactsFragment_to_contactFragment2"
            app:destination="@id/contactFragment" />
    </fragment>
</navigation>